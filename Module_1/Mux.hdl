/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
Not(in=sel,out=selnot);
And(a=a,b=selnot,out=selnotanda);
And(a=sel,b=b,out=selandb);
Or(a=selnotanda,b=selandb,out=out);	
}